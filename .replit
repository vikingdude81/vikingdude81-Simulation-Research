task = "workflow.run"
args = "Bitcoin Predictor"

[[workflows.workflow]]
name = "Bitcoin Predictor"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"

[workflows.workflow.metadata]
outputType = "console"

